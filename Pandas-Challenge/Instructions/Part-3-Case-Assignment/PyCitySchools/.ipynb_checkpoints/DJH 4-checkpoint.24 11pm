    "#craete school_data_complete\n",
    "\n",
    "school_data = pyschools_merge [['School','Enrollment', 'Budget', 'Reading Score', 'Math Score']]\n",
    "\n",
    "\n",
    "# Calculate the total school budget and per capita spending: DONE\n",
    "school_group = school_data.groupby([\"School\"]).mean()\n",
    "school_group['Per Pupil Budget'] = school_group [\"Budget\"]/school_group['Enrollment']\n",
    "\n",
    "school_group\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
